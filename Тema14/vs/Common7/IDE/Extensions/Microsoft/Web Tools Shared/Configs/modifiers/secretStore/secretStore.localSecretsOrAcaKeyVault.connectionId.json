{
  "kind": "secrets.secretStore",
  "restorable": true,
  "model": {
    "secretStore": "@@secretStore@@",
    "secretName": "@@connectionId@@",
    "secretValue": "@@ConnectionValue@@",
    "secretStoreResourceId": "@@SecretStoreResourceId@@",
    "secretExtraData": {
      "valueKind": "Setting"
    },
    "applicableScope": {
      "Local": [
        "LocalSecretsFile"
      ],
      "ContainerApp": [
        "AzureKeyVault"
      ]
    }
  }
}