{
  "kind": "jsonProperty.generic",
  "restorable": true,
  "model": {
    "filePath": "@@propertiesFolder@@/launchSettings.json",
    "key": "profiles/*/environmentVariables",
    "properties": {
      "@@connectionId@@": {
        "value": "@@ConnectionValue@@",
        "action": "replace"
      },
      "AZURE_USERNAME": {
        "value": "@@AzureUsername@@",
        "action": "replace"
      }
    }
  }
}