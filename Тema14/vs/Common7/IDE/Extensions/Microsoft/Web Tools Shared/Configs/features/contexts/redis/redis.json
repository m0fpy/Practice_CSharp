{
  "appliesTo": [
    {
      "when": {
        "kind": "and",
        "operands": [
          {
            "kind": "HasProjectCapability",
            "capability": "DotNetCoreWeb"
          },
          {
            "kind": "not",
            "operand": {
              "kind": "HasTargetFramework",
              "tfmPrefixOrFull": ".NetStandard"
            }
          }
        ]
      },
      "config": "redis.coreweb"
    },
    {
      "when": {
        "kind": "HasProjectCapability",
        "capability": "DotNetCoreWorker"
      },
      "config": "redis.coreweb"
    },
    {
      "when": {
        "kind": "not",
        "operand": {
          "kind": "and",
          "operands": [
            {
              "kind": "HasProjectCapability",
              "capability": "DotNetCoreWeb"
            },
            {
              "kind": "not",
              "operand": {
                "kind": "HasTargetFramework",
                "tfmPrefixOrFull": ".NetStandard"
              }
            }
          ]
        }
      },
      "config": "redis"
    }
  ]
}