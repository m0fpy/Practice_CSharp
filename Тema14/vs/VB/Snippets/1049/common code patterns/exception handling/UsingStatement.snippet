<?xml version="1.0" encoding="UTF-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Оператор Using</Title>
      <Author>Microsoft Corporation</Author>
      <Description>Вставка оператора Using.</Description>
      <Shortcut>Using</Shortcut>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>System</Namespace>
        </Import>
        <Import>
          <Namespace>Microsoft.VisualBasic</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Object>
          <ID>DisposeObject</ID>
          <Type>Object</Type>
          <ToolTip>Замените на имя переменной объекта.</ToolTip>
          <Default>resource</Default>
        </Object>
        <Literal>
          <ID>ObjectType</ID>
          <Type></Type>
          <ToolTip>Замените на оператор Type или New.</ToolTip>
          <Default>New Object</Default>
        </Literal>
      </Declarations>
      <Code Language="VB" Kind="method body"><![CDATA[Using $DisposeObject$ As $ObjectType$

End Using]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
