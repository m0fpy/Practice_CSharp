<?xml version="1.0" encoding="UTF-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Преобразовать строку в массив байтов</Title>
      <Author>Microsoft Corporation</Author>
      <Description>Преобразование строки в массив байтов.</Description>
      <Shortcut>typeStrByte</Shortcut>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>System.Text</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Literal>
          <ID>SampleString</ID>
          <Type>String</Type>
          <ToolTip>Замените на строку.</ToolTip>
          <Default>"StringToConvert"</Default>
        </Literal>
      </Declarations>
      <Code Language="VB" Kind="method body"><![CDATA[dim bytes as Byte() = Encoding.Unicode.GetBytes($SampleString$)]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
