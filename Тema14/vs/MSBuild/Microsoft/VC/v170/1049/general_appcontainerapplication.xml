<Rule Name="ConfigurationGeneral" DisplayName="Общие" PageTemplate="generic" Description="Общие" xmlns="http://schemas.microsoft.com/build/2009/properties">
  <Rule.Categories>
    <Category Name="General" DisplayName="Общие" Description="Общие"/>
    <Category Name="ProjectDefaults" DisplayName="Значения по умолчанию для проекта" Description="Значения по умолчанию для проекта"/>
  </Rule.Categories>
  <Rule.DataSource>
    <DataSource Persistence="ProjectFile" Label="Configuration"/>
  </Rule.DataSource>
  <StringProperty Name="SDKDisplayName" DisplayName="Целевая платформа" Description="Текущая целевая платформа проекта" ReadOnly="true" Category="General" F1Keyword="VC.Project.VCConfiguration.AppSupport">
                  <StringProperty.DataSource>
                    <DataSource Persistence="ProjectFile" Label=""/>
                  </StringProperty.DataSource>
  </StringProperty>
  <DynamicEnumProperty Name="WindowsTargetPlatformVersion" DisplayName="Версия целевой платформы" Description="Версия целевой платформы. Выберите 10.0, чтобы использовать последнюю установленную версию пакета SDK для Windows 10." Category="General" EnumProvider="PlatformVersionsEx" Visible="true" F1Keyword="VC.Project.VCConfiguration.TargetPlatformVersion">
    <DynamicEnumProperty.DataSource>
      <DataSource Persistence="ProjectFile" Label="Globals" SourceOfDefaultValue="AfterContext" HasConfigurationCondition="False"/>
    </DynamicEnumProperty.DataSource>
  </DynamicEnumProperty>
  <DynamicEnumProperty Name="WindowsTargetPlatformMinVersion" DisplayName="Минимальная версия целевой платформы" Description="Минимальная версия возможной целевой платформы приложения." Category="General" EnumProvider="PreviousPlatformVersions" Visible="true" F1Keyword="VC.Project.VCConfiguration.TargetPlatformVersion">
    <DynamicEnumProperty.DataSource>
      <DataSource Persistence="ProjectFile" Label="Globals" SourceOfDefaultValue="AfterContext" HasConfigurationCondition="False"/>
    </DynamicEnumProperty.DataSource>
  </DynamicEnumProperty>
  <StringProperty Name="OutDir" DisplayName="Выходной каталог" Description="Указывает относительный путь к выходному каталогу файлов; может включать в себя переменные окружения." Category="General" Default="$(SolutionDir)$(Configuration)\" Subtype="folder" F1Keyword="VC.Project.VCConfiguration.OutputDirectory">
                  <StringProperty.DataSource>
                     <DataSource Persistence="ProjectFile" Label=""/>
                  </StringProperty.DataSource>
  </StringProperty>
  <StringProperty Name="IntDir" DisplayName="Промежуточный каталог" Description="Указывает относительный путь к промежуточному каталогу файлов; может включать в себя переменные окружения." Category="General" Default="$(Configuration)\" Subtype="folder" F1Keyword="VC.Project.VCConfiguration.IntermediateDirectory">
                  <StringProperty.DataSource>
                     <DataSource Persistence="ProjectFile" Label=""/>
                  </StringProperty.DataSource>
  </StringProperty>
  <StringProperty Name="TargetName" DisplayName="Имя целевого объекта" Description="Задает расширение файла, создаваемого проектом." F1Keyword="VC.Project.VCConfiguration.TargetName" Category="General" Default="$(ProjectName)">
                  <StringProperty.DataSource>
                     <DataSource Persistence="ProjectFile" Label=""/>
                  </StringProperty.DataSource>
  </StringProperty>
  <StringProperty Name="TargetExt" DisplayName="Конечное расширение" Description="Задает расширение файла, создаваемого проектом. (Например: .exe или .dll)" F1Keyword="VC.Project.VCConfiguration.TargetExt" Category="General">
                  <StringProperty.DataSource>
                     <DataSource Persistence="ProjectFile" Label=""/>
                  </StringProperty.DataSource>
  </StringProperty>
  <StringListProperty Name="ExtensionsToDeleteOnClean" DisplayName="Расширения для удаления при очистке" Description="Спецификация (с использованием подстановочных знаков и точки с запятой в качестве разделителя) файлов в промежуточном каталоге, которые должны быть удалены при очистке или повторном построении." Category="General" Separator=";" F1Keyword="VC.Project.VCConfiguration.DeleteExtensionsOnClean">
                      <StringListProperty.DataSource>
                            <DataSource Persistence="ProjectFile" Label=""/>
                      </StringListProperty.DataSource>
  </StringListProperty>
  <StringProperty Name="BuildLogFile" DisplayName="Файл журнала сборки" Description="Определяет файл журнала сборки, в который будет вестись запись, если ведение журнала включено." F1Keyword="VC.Project.VCConfiguration.BuildLogFile" Category="General" Default="$(ProjectDir)\$(MSBuildProjectName).log" Subtype="file">
                  <StringProperty.DataSource>
                     <DataSource Persistence="ProjectFile" ItemType="BuildLog" PersistedName="Path"/>
                  </StringProperty.DataSource>
  </StringProperty>
  <!-- There is no InheritProjectPropertySheets in the project file. The property sheets will be expressed as explicit Imports.
  <StringListProperty Name="InheritProjectPropertySheets" DisplayName="Inherit Project Property Sheets"
                      Description="Specifies the project property sheets this configuration or property sheet inherits from."
                      Category="General" Separator=";" Subtype="file" /> -->
  <DynamicEnumProperty Name="PlatformToolset" DisplayName="Набор инструментов платформы" EnumProvider="Toolsets" Description="Задает набор инструментов, используемый для сборки текущей конфигурации; если не указан, используется набор инструментов по умолчанию." F1Keyword="VC.Project.VCConfiguration.PlatformToolset" Category="General"/>
  <EnumProperty Name="ConfigurationType" DisplayName="Тип конфигурации" Description="Определяет тип выходных файлов, создаваемый этой конфигурацией." Category="ProjectDefaults" F1Keyword="VC.Project.VCConfiguration.ConfigurationType">
    <EnumValue Name="Application" DisplayName="Приложение (.exe)" Description="Приложение (.exe)"/>
    <EnumValue Name="DynamicLibrary" DisplayName="Динамическая библиотека (.dll)" Description="Динамическая библиотека (.dll)"/>
    <EnumValue Name="StaticLibrary" DisplayName="Статическая библиотека (.lib)" Description="Статическая библиотека (.lib)"/>
  </EnumProperty>
  <EnumProperty Name="WholeProgramOptimization" DisplayName="Оптимизация всей программы" Description="Указывает, что программа будет оптимизирована через границы obj-файлов; работает наиболее эффективно при включенных оптимизациях C/C++." Category="ProjectDefaults" F1Keyword="VC.Project.VCConfiguration.WholeProgramOptimization">
    <EnumValue Name="false" DisplayName="Без оптимизации всей программы" Description="Без оптимизации всей программы"/>
    <EnumValue Name="true" DisplayName="Использовать создание кода во время компоновки" Description="Использовать создание кода во время компоновки"/>
  </EnumProperty>
  <BoolProperty Name="WindowsAppContainer" DisplayName="Поддержка приложений для Магазина Windows" Description="Включить поддержку приложений для Магазина Windows в этом проекте." ReadOnly="true" Category="ProjectDefaults" F1Keyword="VC.Project.VCConfiguration.AppSupport"/>
  <BoolProperty Name="DesktopCompatible" DisplayName="Совместимый с классическими приложениями Windows" Description="Разрешить использование этого выходного файла проекта в классических приложениях Windows." Category="ProjectDefaults" F1Keyword="VC.Project.VCConfiguration.DesktopCompatible"/>
  <EnumProperty Name="CharacterSet" DisplayName="Набор символов" Description="Указывает, какой набор знаков следует использовать компилятору; актуально при локализации." Category="ProjectDefaults" Visible="false" F1Keyword="VC.Project.VCConfiguration.CharacterSet">
    <EnumValue Name="Unicode" DisplayName="Использовать набор символов Юникода" Description="Использовать набор символов Юникода"/>
  </EnumProperty>
  <BoolProperty Name="UseDebugLibraries" DisplayName="Использовать отладочные библиотеки" Description="Задает использование отладочных библиотек и переключателей в данной конфигурации" Category="ProjectDefaults" Visible="false">
  </BoolProperty>
  <StringProperty Name="_ProjectFileVersion" DisplayName="Версия файла проекта" Description="Задает версию инструмента преобразования, используемого для создания этого файла проекта." Category="General" Visible="false">
    <StringProperty.DataSource>
      <DataSource Persistence="ProjectFile"/>
    </StringProperty.DataSource>
  </StringProperty>
  <StringProperty Name="ProjectGuid" Visible="False">
    <StringProperty.DataSource>
      <DataSource Persistence="ProjectFile" Label="Globals" HasConfigurationCondition="False"/>
    </StringProperty.DataSource>
  </StringProperty>
  <DynamicEnumProperty Name="TargetFrameworkMoniker" DisplayName="Моникер целевой платформы" EnumProvider="TargetFrameworkMoniker" Visible="false"/>

  <EnumProperty Name="UseOfMfc" Category="ProjectDefaults" Visible="false">
    <EnumValue Name="false"/>
    <EnumValue Name="Static"/>
    <EnumValue Name="Dynamic"/>
  </EnumProperty>
  <EnumProperty Name="UseOfAtl" Visible="false">
    <EnumValue Name="false"/>
    <EnumValue Name="Static"/>
    <EnumValue Name="Dynamic"/>
  </EnumProperty>
  <EnumProperty Name="CLRSupport" Visible="False">
    <EnumValue Name="false"/>
    <EnumValue Name="true"/>
    <EnumValue Name="Pure"/>
    <EnumValue Name="Safe"/>
  </EnumProperty>
  <StringProperty Name="TargetPath" Visible="False"/>
  <StringProperty Name="MSBuildProjectDirectory" Visible="false"/>
  <StringProperty Name="PackageAction" Visible="false" Description="Целевой объект MSBuild, используемый при упаковке проекта."/>
  <StringProperty Name="DefaultContentType" Visible="false" Description="Имя типа содержимого по умолчанию, используемое при добавлении файлов."/>
  <StringProperty Name="VCInstallDir" Visible="false"/>
  <StringProperty Name="VCIDEInstallDir" Visible="false"/>
  <StringProperty Name="VSInstallDir" Visible="false"/>
  <StringProperty Name="Platform" Visible="false"/>
  <StringProperty Name="Configuration" Visible="false"/>
  <StringProperty Name="DebuggerSymbolsPath" Visible="false"/>
  <StringProperty Name="AssemblySearchPaths" Visible="false"/>
  <StringProperty Name="DefaultPlatformToolset" Visible="false"/>
  <BoolProperty Name="EnableDesignTimeBuild" Visible="false"/>
  <BoolProperty Name="AppContainerApplication" Visible="false"/>
  <EnumProperty Name="TargetRuntime" Visible="false">
    <EnumValue Name="Managed"/>
    <EnumValue Name="Native"/>
    <EnumValue Name="AppHost" DisplayName="Приложение Windows"/>
  </EnumProperty>
  <StringProperty Name="AddItemTemplateGuid" Visible="false"/>
  <BoolProperty Name="ContainsStartupTask" Visible="false"/>
</Rule>
