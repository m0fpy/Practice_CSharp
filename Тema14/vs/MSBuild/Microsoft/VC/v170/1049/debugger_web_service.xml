<Rule Name="WebServiceDebugger" DisplayName="Отладчик веб-служб" PageTemplate="debugger" Description="Параметры отладчика веб-служб" xmlns="http://schemas.microsoft.com/build/2009/properties">
  <Rule.DataSource>
    <DataSource Persistence="UserFile"/>
  </Rule.DataSource>
  <StringProperty Name="WebServiceDebuggerHttpUrl" DisplayName="HTTP URL" Description="Определяет URL-адрес для проекта." F1Keyword="VC.Project.IVCWebSvcDebugPageObject.HttpUrl"/>
  <EnumProperty Name="WebServiceDebuggerDebuggerType" DisplayName="Тип отладчика" Description="Определение тип используемого отладчика. При выборе значения &quot;Авто&quot; тип отладчика будет выбран в зависимости от содержимого exe-файла." F1Keyword="VC.Project.IVCWebSvcDebugPageObject.DebuggerType">
    <EnumValue Name="NativeOnly" DisplayName="Только машинный код" Description="Только машинный код"/>
    <EnumValue Name="ManagedOnly" DisplayName="Только управляемый код" Description="Только управляемый код"/>
    <EnumValue Name="Mixed" DisplayName="Смешанный код" Description="Смешанный код"/>
    <EnumValue Name="Auto" DisplayName="Авто" Description="Авто"/>
    <EnumValue Name="Script" DisplayName="Скрипт" Description="Скрипт"/>
  </EnumProperty>
  <BoolProperty Name="WebServiceDebuggerSQLDebugging" DisplayName="Отладка SQL" Description="Присоединение SQL-отладчика." F1Keyword="VC.Project.IVCWebSvcDebugPageObject.SQLDebugging"/>
  <BoolProperty Name="UseLegacyManagedDebugger" Visible="false"/>
</Rule>
