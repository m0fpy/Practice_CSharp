                                                                                                <xs:enumeration value="PeerTrust"/>
                                                                                                <xs:enumeration value="ChainTrust"/>
                                                                                                <xs:enumeration value="PeerOrChainTrust"/>
                                                                                                <xs:enumeration value="Custom"/>
