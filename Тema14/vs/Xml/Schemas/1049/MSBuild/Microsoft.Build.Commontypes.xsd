    <!-- ============================ -->
    <!-- Fast up-to-date check items. -->
    <!-- ============================ -->

    <xs:element name="UpToDateCheckInput" substitutionGroup="msb:Item">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="UpToDateCheckInputDescription" _locComment="" -->Определяет элемент, который будет считаться входными данными проекта для быстрой проверки актуальности.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:attribute name="Set">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="UpToDateCheckInput_Set" _locComment="" -->Необязательные группы входных и выходных данных, которые должны рассматриваться в изолированном режиме во время сборки. Удобно, если сборка включает несколько этапов дискретной компиляции или транспиляции. Если требуется несколько наборов, применяется разделение точкой с запятой.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Kind">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="UpToDateCheckInput_Kind" _locComment="" -->Необязательный идентификатор для этого элемента, который позволяет пропустить его при быстрой проверке актуальности с помощью глобального свойства.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="UpToDateCheckOutput" substitutionGroup="msb:Item">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="UpToDateCheckOutputDescription" _locComment="" -->Определяет элемент, который будет считаться выходными данными проекта для быстрой проверки актуальности.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:attribute name="Set">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="UpToDateCheckOutput_Set" _locComment="" -->Необязательные группы входных и выходных данных, которые должны рассматриваться в изолированном режиме во время сборки. Удобно, если сборка включает несколько этапов дискретной компиляции или транспиляции. Если требуется несколько наборов, применяется разделение точкой с запятой.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Kind">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="UpToDateCheckOutput_Kind" _locComment="" -->Необязательный идентификатор для этого элемента, который позволяет пропустить его при быстрой проверке актуальности с помощью глобального свойства.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="UpToDateCheckBuilt" substitutionGroup="msb:Item">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="UpToDateCheckBuiltDescription" _locComment="" -->Определяет элемент, который будет считаться выходными данными проекта для быстрой проверки актуальности с необязательными соответствующими входными данными с использованием метаданных Original. Если указаны метаданные Original, входные и выходные данные считаются находящимися в изолированном режиме. Удобно при копировании (и, возможно, сопутствующем преобразовании) одного файла во время сборки.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:attribute name="Set">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="UpToDateCheckBuilt_Set" _locComment="" -->Необязательные группы входных и выходных данных, которые должны рассматриваться в изолированном режиме во время сборки. Удобно, если сборка включает несколько этапов дискретной компиляции или транспиляции. Если требуется несколько наборов, применяется разделение точкой с запятой.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Kind">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="UpToDateCheckBuilt_Kind" _locComment="" -->Необязательный идентификатор для этого элемента, который позволяет пропустить его при быстрой проверке актуальности с помощью глобального свойства.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Original">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="UpToDateCheckBuilt_Original" _locComment="" -->Необязательное расположение входного элемента, создающего эти выходные данные. Удобно при копировании файла (и, возможно, сопутствующем преобразовании) во время сборки. Если используется несколько входных и/или выходных данных, вместо этого применяйте метаданные Set.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

