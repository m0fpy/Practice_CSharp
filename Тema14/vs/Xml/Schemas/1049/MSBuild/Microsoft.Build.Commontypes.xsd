                        </xs:element>
                        <xs:element name="ExpectedTargetSizes" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation><!-- _locID_text="AppxManifestImageFileNameQuery_ExpectedScaleDimensions" _locComment="" -->Разделенный точками с запятой список ожидаемых целевых размеров.</xs:documentation>
