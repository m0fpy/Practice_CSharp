<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://schemas.microsoft.com/developer/msbuild/2003" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msb="http://schemas.microsoft.com/developer/msbuild/2003" elementFormDefault="qualified">

    <!-- =================== INCLUDE COMMON SCHEMA =========================== -->
    <xs:include schemaLocation="Microsoft.Build.Core.xsd"/>

    <!-- ======================== ITEMS =====================================-->
    <!-- Possible Types include SimpleItemType (no meta-data subelements), GenericItemType (any meta-data), or something more specific.-->
    <xs:element name="Reference" substitutionGroup="msb:Item">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="Reference" _locComment="" -->Ссылка на сборку</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:choice>
                            <xs:element name="HintPath">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_HintPath" _locComment="" -->Относительный или абсолютный путь к сборке (необязательный)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Name">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_Name" _locComment="" -->Отображаемое понятное имя (необязательный)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="FusionName">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_FusionName" _locComment="" -->Fusion-имя сборки (необязательный)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="SpecificVersion" type="msb:boolean">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_SpecificVersion" _locComment="" -->Должна быть создана ссылка только на версию в fusion-имени или нет (необязательный, логический)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Aliases">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_Aliases" _locComment="" -->Псевдонимы для ссылки (необязательный)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Private" type="msb:boolean">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_Private" _locComment="" -->Должна ссылка копироваться в выходную папку или нет (необязательный, логический)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                          <xs:element name="EmbedInteropTypes" type="msb:boolean">
                            <xs:annotation>
                              <xs:documentation><!-- _locID_text="Reference_EmbedInteropTypes" _locComment="" -->Указывает, требуется ли внедрить типы из этой ссылки в конечную сборку — только сборки взаимодействия (необязательный, логический)</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="RequiredTargetFramework">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_RequiredTargetFramework" _locComment="" -->Минимальная используемая версия .Net Framework для ссылки на данную сборку</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:choice>
                    </xs:sequence>
                    <!-- redefine Include just to give a specific description -->
                    <xs:attribute name="Include" type="xs:string" use="optional">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="Reference_Include" _locComment="" -->Имя сборки или имя файла сборки</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="SDKReference" substitutionGroup="msb:Item">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="SDKReference" _locComment="" -->Ссылка на SDK для расширений</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:choice>
                            <xs:element name="Name">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_Name" _locComment="" -->Отображаемое понятное имя (необязательный)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:choice>
                    </xs:sequence>
                    <!-- redefine Include just to give a specific description -->
                    <xs:attribute name="Include" type="xs:string" use="optional">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="Reference_Include" _locComment="" -->Имя и моникер версии, представляющие SDK расширения</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="COMReference" substitutionGroup="msb:Item">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="COMReference" _locComment="" -->Ссылка на COM-компонент</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:choice>
                            <xs:element name="Name">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_Name" _locComment="" -->Отображаемое понятное имя (необязательный)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Guid">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_Guid" _locComment="" -->Идентификатор GUID в формате {00000000-0000-0000-0000-000000000000}</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="VersionMajor">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_VersionMajor" _locComment="" -->Старшая часть номера версии</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="VersionMinor">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_VersionMinor" _locComment="" -->Младшая часть номера версии</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Lcid">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_Lcid" _locComment="" -->Идентификатор языка</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="WrapperTool">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_WrapperTool" _locComment="" -->Инструмент создания оболочек, например tlbimp</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Isolated" type="msb:boolean">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_Isolated" _locComment="" -->Является изолированным или нет (логический)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                          <xs:element name="EmbedInteropTypes" type="msb:boolean">
                            <xs:annotation>
                              <xs:documentation><!-- _locID_text="COMReference_EmbedInteropTypes" _locComment="" -->Указывает, требуется ли внедрить типы из этой ссылки в конечную сборку — только сборки взаимодействия (необязательный, логический)</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:choice>
                    </xs:sequence>
                    <!-- redefine Include just to give a specific description -->
                    <xs:attribute name="Include" type="xs:string" use="optional">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="COMReference_Include" _locComment="" -->Имя COM-компонента</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="COMFileReference" substitutionGroup="msb:Item">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:choice>
                            <xs:element name="WrapperTool"/>
                        </xs:choice>
                    </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="PackageReference" substitutionGroup="msb:Item">
    <xs:annotation>
      <xs:documentation>
        <!-- _locID_text="PackageReference" _locComment="" -->Ссылка на пакет
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:choice>
              <xs:element name="IncludeAssets">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="PackageReference_IncludeAssets" _locComment="" -->Ресурсы, включаемые из этой ссылки
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ExcludeAssets">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="PackageReference_ExcludeAssets" _locComment="" -->Ресурсы, исключаемые из этой ссылки
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PrivateAssets">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="PackageReference_PrivateAssets" _locComment="" -->Ресурсы, являющиеся частными в этой ссылке
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="NoWarn">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="PackageReference_NoWarn" _locComment="" -->Список игнорируемых кодов предупреждений через точку с запятой (например, NU1605)
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="GeneratePathProperty">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="PackageReference_GeneratePathProperty" _locComment="" -->Установите значение true, чтобы создать свойство Pkg*, которое указывает на восстановленное расположение содержимого пакета NuGet. Если значение не установлено, свойство не создается.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="VersionOverride">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="PackageReference_VersionOverride" _locComment="" -->При использовании централизованного управления пакетами (CPM) переопределяет централизованно определенную версию для этого пакета.  Если проект не использует CPM, этот элемент не оказывает никакого влияния.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:choice>
          </xs:sequence>
          <xs:attribute name="Include" type="xs:string">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Include" _locComment="" -->Имя пакета
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Version" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Version" _locComment="" -->Версия зависимости
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="IncludeAssets" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Attribute_IncludeAssets" _locComment="" -->Включаемые ресурсы из этой ссылки
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="ExcludeAssets" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Attribute_ExcludeAssets" _locComment="" -->Исключаемые ресурсы из этой ссылки
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="PrivateAssets" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Attribute_PrivateAssets" _locComment="" -->Закрытые ресурсы в этой ссылке
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="NoWarn" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Attribute_NoWarn" _locComment="" -->Список игнорируемых кодов предупреждений через точку с запятой (например, NU1605)
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="GeneratePathProperty" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Attribute_GeneratePathProperty" _locComment="" -->Установите значение true, чтобы создать свойство Pkg*, которое указывает на восстановленное расположение содержимого пакета NuGet
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="VersionOverride" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Attribute_VersionOverride" _locComment="" -->При использовании централизованного управления пакетами (CPM) переопределяет централизованно определенную версию для этого пакета.  Если проект не использует CPM, этот атрибут не оказывает никакого влияния.
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="FrameworkReference" substitutionGroup="msb:Item">
    <xs:annotation>
      <xs:documentation>
        <!-- _locID_text="FrameworkReference" _locComment="" -->Ссылка на общую платформу.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:choice>
              <xs:element name="IsTrimmable">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="FrameworkReference_IsTrimmable" _locComment="" -->Определяет, могут ли ресурсы среды выполнения этой общей платформы обрезаться компоновщиком IL Linker (если PublishTrimmed имеет значение true, т. е. истина).
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="TargetLatestRuntimePatch">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="FrameworkReference_TargetLatestRuntimePatch" _locComment="" -->Определяет, будет ли приложение предназначено для новейшей версии среды выполнения. По умолчанию для автономных приложений используется значение true (истина), для других — false (ложь).
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:choice>
          </xs:sequence>
          <xs:attribute name="IsTrimmable" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="FrameworkReference_Attribute_IsTrimmable" _locComment="" -->Определяет, могут ли ресурсы среды выполнения этой общей платформы обрезаться компоновщиком IL Linker (если PublishTrimmed имеет значение true, т. е. истина).
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="TargetLatestRuntimePatch" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="FrameworkReference_Attribute_TargetLatestRuntimePatch" _locComment="" -->Определяет, будет ли приложение предназначено для новейшей версии среды выполнения. По умолчанию для автономных приложений используется значение true (истина), для других — false (ложь).
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="DotNetCliToolReference" substitutionGroup="msb:Item">
    <xs:annotation>
      <xs:documentation>
        <!-- _locID_text="DotNetCliToolReference" _locComment="" -->Инструмент CLI, который пользователю требуется восстановить в контексте проекта
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:choice>
              <xs:element name="IncludeAssets">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="DotNetCliToolReference_IncludeAssets" _locComment="" -->Ресурсы, включаемые из этой ссылки
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ExcludeAssets">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="DotNetCliToolReference_ExcludeAssets" _locComment="" -->Ресурсы, исключаемые из этой ссылки
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PrivateAssets">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="DotNetCliToolReference_PrivateAssets" _locComment="" -->Ресурсы, являющиеся частными в этой ссылке
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:choice>
          </xs:sequence>
          <xs:attribute name="Include" type="xs:string">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="DotNetCliToolReference_Include" _locComment="" -->Имя пакета инструмента. Может отличаться от связанного имени пакета для ссылки.
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Version" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="DotNetCliToolReference_Version" _locComment="" -->Версия зависимости
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="Xdcmake" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:choice>
              <xs:element name="SuppressStartupBanner"/>
              <xs:element name="AdditionalDocumentFile"/>
              <xs:element name="DocumentLibraryDependencies"/>
            </xs:choice>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="Bscmake" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:choice>
              <xs:element name="PreserveSBR"/>
              <xs:element name="AdditionalOptions"/>
              <xs:element name="SuppressStartupBanner"/>
              <xs:element name="OutputFile"/>
            </xs:choice>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="ClCompile" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="PrecompiledHeader" minOccurs="0">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:string">
                    <xs:attribute name="Condition" use="optional"></xs:attribute>
                </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="AdditionalIncludeDirectories"/>
            <xs:element name="AdditionalUsingDirectories"/>
            <xs:element name="CompileAsManaged">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:string">
                    <xs:attribute name="Condition" use="optional"></xs:attribute>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="ErrorReporting"/>
            <xs:element name="WarningLevel"/>
            <xs:element name="MinimalRebuild"/>
            <xs:element name="DebugInformationFormat"/>
            <xs:element name="PreprocessorDefinitions"/>
            <xs:element name="Optimization"/>
            <xs:element name="BasicRuntimeChecks"/>
            <xs:element name="RuntimeLibrary"/>
            <xs:element name="FunctionLevelLinking"/>
            <xs:element name="FloatingPointModel "/>
            <xs:element name="IntrinsicFunctions"/>
            <xs:element name="PrecompiledHeaderFile"/>
            <xs:element name="MultiProcessorCompilation"/>
            <xs:element name="UseUnicodeForAssemblerListing"/>
            <xs:element name="UndefinePreprocessorDefinitions"/>
            <xs:element name="StringPooling"/>
            <xs:element name="BrowseInformation"/>
            <xs:element name="FloatingPointExceptions"/>
            <xs:element name="CreateHotpatchableImage"/>
            <xs:element name="RuntimeTypeInfo"/>
            <xs:element name="OpenMPSupport"/>
            <xs:element name="CallingConvention"/>
            <xs:element name="DisableSpecificWarnings"/>
            <xs:element name="ForcedIncludeFiles"/>
            <xs:element name="ForcedUsingFiles"/>
            <xs:element name="ShowIncludes"/>
            <xs:element name="UseFullPaths"/>
            <xs:element name="OmitDefaultLibName"/>
           <xs:element name="TreatSpecificWarningsAsErrors"/>
         </xs:choice>
       </xs:extension>
      </xs:complexContent>
     </xs:complexType>
   </xs:element>
  <xs:element name="ClInclude" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="AdditionalIncludeDirectories" minOccurs="0"/>
          </xs:choice>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="Midl" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="MkTypLibCompatible"/>
            <xs:element name="ValidateAllParameters" type="msb:boolean"/>
            <xs:element name="PreprocessorDefinitions"/>
            <xs:element name="TypeLibraryName"/>
            <xs:element name="ErrorCheckRefPointers"/>
            <xs:element name="ErrorCheckStubData"/>
            <xs:element name="ErrorCheckBounds"/>
            <xs:element name="HeaderFileName"/>
            <xs:element name="ErrorCheckEnumRange"/>
            <xs:element name="ErrorCheckAllocations"/>
            <xs:element name="WarnAsError"/>
            <xs:element name="GenerateTypeLibrary"/>
            <xs:element name="AdditionalIncludeDirectories"/>
            <xs:element name="IgnoreStandardIncludePath"/>
            <xs:element name="SuppressStartupBanner"/>
            <xs:element name="DefaultCharType"/>
            <xs:element name="TargetEnvironment"/>
            <xs:element name="GenerateStublessProxies"/>
            <xs:element name="SuppressCompilerWarnings"/>
            <xs:element name="ApplicationConfigurationMode"/>
            <xs:element name="LocaleID"/>
            <xs:element name="OutputDirectory"/>
            <xs:element name="DllDataFileName"/>
            <xs:element name="InterfaceIdentifierFileName"/>
            <xs:element name="ProxyFileName"/>
            <xs:element name="GenerateClientFiles"/>
            <xs:element name="GenerateServerFiles"/>
            <xs:element name="ClientStubFile"/>
            <xs:element name="ServerStubFile"/>
            <xs:element name="TypeLibFormat"/>
